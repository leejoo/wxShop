.container {
    background-color:#F2f2f2;
}
.address-list{
    width: 100%;
    background-color: #fff;
    margin-top: 20rpx;
}
.address-list .a-address{
    width: 720rpx;
    margin-left: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #eee;
    flex-direction: column;
}
.a-address .left-text{
    width: 610rpx;
    box-sizing: border-box;
    padding-left: 58rpx;
}
.a-address .left-text .name-tel{
    margin-bottom: 20rpx;
}
.a-address .left-text .address-box{
    font-size:24rpx;
    color:#888888;
    line-height:36rpx;
}
.a-address .meta-box{
    display:flex;
    justify-content: space-around;
    padding-top: 10rpx;
    box-sizing: border-box;
}
.address-item{
  display: flex;
  flex-direction: row;
  justify-content:flex-start;
}
