<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">下单成功</view>
            <view class="weui-msg__desc">订单编号：<navigator url="" class="weui-msg__link">{{detail.ordernum}}</navigator></view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap='toAddOrder'>继续下单</button>
                <button class="weui-btn" bindtap='toMyOrder' type="default">我的订单</button>
            </view>
        </view>
    </view>
</view>