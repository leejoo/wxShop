.show-address{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  width: 784rpx;
  box-sizing: border-box;
  padding-left: 0rpx;
}
.def-address{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.show-address .name-tel {
  font-size: 28rpx;
  color: #000;
  padding: 30rpx 0 20rpx 0;
}

.show-address .addr-text {
  font-size: 24rpx;
  color: #888;
  padding-bottom: 34rpx;
  line-height: 36rpx;
}

.m70 {
  margin-bottom: 70px;
}
.goods {
  background: #fff;
  border-top: 1px solid #ddd;
  margin-bottom: 5px;
  padding: 5px 5px 0 5px;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  flex-direction: row;
}

.goods image {
  width: 55px;
  height: 55px;
  margin-left: 20rpx;
}

.good-cont {
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: space-between;
  color: #555;
  font-size: 12px;
  width: 100rpx;
  padding: 5px;
	margin-left: 10rpx;
  margin-right: 320rpx;
}

.goods-navigator {
  display: flex;
  font-size: 14px;
  flex: 1;
  width: 200rpx;
}

.goods-navigator text {
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  height: 3em
}

.good-price {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex: 3;
}

.price {
  display: flex;
  font-size: 16px;
  color: #ec5151;
  height: 25px;
  align-items: center;
}

.desk{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  overflow: hidden;
  margin-left: 15px;
  border-bottom: 1px solid #e1e1e1;
  height: 50px;
  line-height: 50px;
  padding-right: 15px;
}

.desk>text {
  float: left;
  color: #3c3c3c;
  font-size: 18px;
}

.desk .code{
  height: 60rpx;
  width: 60rpx;
  vertical-align: middle;
  margin-top: 18rpx;
}
.weui-navbar{
  z-index: 9999;
  background: #f60;
  color: #fff;
}
.weui-tab__content{
    text-align: center;
    height: 900rpx;
}
.item {
  margin-bottom: 40rpx;
}

.item .item-top {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 750rpx;
  height: 80rpx;
  box-sizing: border-box;
  padding: 0 10rpx;
  border-top: 1rpx solid #f4f4f4;
  border-bottom: 1rpx solid #f4f4f4;
}
