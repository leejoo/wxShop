@import "/style/weui.wxss";
page, .page, .page__bd {
  height: 100%;
}

.page__bd {
  padding-bottom: 0;
}

.weui-tab__content {
  text-align: left;
}

page {
  background: #fff;
}

.takeOrder .place {
  height: 50px;
  line-height: 50px;
  margin-left: 15px;
  background: #ececec;
  width: 100%;
}

.food-detail {
  overflow: hidden;
  margin-left: 15px;
  border-bottom: 1px solid #e1e1e1;
  height: 50px;
  line-height: 50px;
  padding-right: 15px;
}

.food-detail>text {
  float: left;
  color: #3c3c3c;
  font-size: 18px;
}

.food-detail>view {
  float: right;
}

.food-detail>view text {
  font-size: 15px;
  color: #999;
}

.nobd {
  border: 0 none;
}

.takeBill {
  height: 50px;
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  border-top: 1px solid #e6e6e6;
  z-index: 2;
  background: #f60;
  color: #fff;
}

.takeBill .box-left {
  float: left;
  line-height: 50px;
  padding-left: 15px;
}

.takeBill .box-right {
  float: right;
  background: #fa4d22;
  font-size: 18px;
  color: #fff;
  width: 120px;
  border-radius: 0;
  height: 50px;
}

.takeBill .box-left text {
  color: #fff;
}

.food-detail>view {
  margin-top: 13px;
}

.food-detail>view input {
  text-align: right;
}

.food-detail input, .food-detail textarea {
  max-width: 244px;
}

.nomt>view {
  margin-top: 0;
}

.hidden {
  display: none !important;
}

.address-box {
  width: 100%;
  margin: 20rpx 0;
}

.mypos {
  width: 80rpx;
  height: 80rpx;
  margin-left: 15rpx;
  margin-top: 50rpx;
}

.add-addres {
  width: 100%;
}

.address-hor {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

